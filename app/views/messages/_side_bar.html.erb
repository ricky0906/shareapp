<div class="side-bar-header">
  <div class="header-name">
    <%= link_to current_user.nickname, user_path(current_user) %>
  </div>
</div>

<div class="rooms">
  <% Room.all.each do |room| %>
    <div class="room">
      <div class="room-name">
        <%= link_to room.name, room_messages_path(room) %>
      </div>
    </div>
  <% end %>
</div>